{
    "name": "swissup/module-geoip",
    "description": "Magento 2 module for geo location detecting by ip",
    "keywords": [
        "geoip",
        "address detection",
        "country",
        "city",
        "postcode"
    ],
    "type": "magento2-module",
    "version": "1.5.4",
    "license": "OSL-3.0",
    "require": {
        "swissup/module-checkout": "^1.5.1",
        "geoip2/geoip2":"~2.0"
    },
    "autoload": {
        "files": [ "registration.php" ],
        "psr-4": {
            "Swissup\\Geoip\\": ""
        }
    },
    "extra": {
        "swissup": {
            "links": {
                "docs"     : "http://docs.swissuplabs.com/m2/extensions/geoip/",
                "changelog": "http://docs.swissuplabs.com/m2/extensions/geoip/changelog/"
            }
        },
        "marketplace": {
            "gallery": [
                "https://docs.swissuplabs.com/images/marketplace/module-geoip.png"
            ],
            "links": {
                "docs": "https://docs.swissuplabs.com/m2/extensions/geoip/",
                "changelog": "https://docs.swissuplabs.com/m2/extensions/geoip/changelog/"
            }
        }
    }
}
